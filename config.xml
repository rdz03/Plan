<?xml version='1.0' encoding='utf-8'?>
<widget id="com.plantcare" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>plantcare</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="estaciones.html" />
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" version="1" />
